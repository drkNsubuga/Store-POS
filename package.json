  "private": true,
  "main": "start.js",
  "build": {
    "appId": "01",
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "async": "^3.2.4",
    "body-parser": "^1.20.1",
    "btoa": "^1.2.1",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "html2canvas": "^1.4.1",
    "http": "0.0.0",
    "https": "^1.0.0",
    "is-port-reachable": "^4.0.0",
    "jquery": "^3.6.3",
    "jsbarcode": "^3.11.5",
    "jspdf": "^2.5.1",
    "macaddress": "^0.5.3",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "nedb": "^1.8.0",
    "nodemon": "^2.0.20",
    "notiflix": "^3.2.5",
    "path": "^0.12.7",
    "print-js": "^1.6.0",
    "socket.io": "^4.5.4",
    "sweetalert2": "^11.6.16",
    "xmlhttprequest": "^1.8.0"
  },
  "scripts": {
    "electron": "cross-env NODE_ENV=dev nodemon --exec \"\"electron .\"\"",
    "electron-build": "electron-builder",
    "package-win": "electron-packager . POS --overwrite --asar=false --platform=win32 --arch=x64 --icon=assets/images/icon.ico --prune=true --out=release-builds --version-string.ProductName=\"POS\"",
    "create-installer-win": "node installers/windows/createinstaller.js",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/publisher-github": "^6.0.4",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-is-dev": "^2.0.0",
    "nodemon": "^1.19.3"
  }
}
